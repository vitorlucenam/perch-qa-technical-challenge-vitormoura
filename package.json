{
  "name": "cypress-challenge",
  "version": "1.0.0",
  "description": "Challenge de Cypress Parallelization con Cucumber y TypeScript.",
  "scripts": {
    "start": "react-scripts start",
    "test": "cypress run",
    "test:parallel": "cypress run --parallel --record --key 47298b7b-69d6-42f5-a8da-9b9ed90784e4",
    "cypress:open": "cypress open"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.14.2",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@cypress/browserify-preprocessor": "^3.0.0",
    "@cypress/webpack-preprocessor": "^5.0.0",
    "@types/cucumber": "^7.0.2",
    "@types/node": "^20.4.8",
    "cypress": "^14.0.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "multiple-cucumber-html-reporter": "^3.9.2",
    "typescript": "^5.1.6"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/integration/step_definitions",
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/integration/step_definitions",
    "failFast": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
